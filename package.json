{
  "name": "wala-whatsapp-like-app",
  "version": "1.0.0",
  "description": "WhatsApp-like Application with DES encryption and RSA key exchange",
  "main": "server/index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "node-rsa": "^1.1.1",
    "crypto-js": "^4.1.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "chat",
    "encryption",
    "websocket",
    "react",
    "nodejs"
  ],
  "author": "WALA Team",
  "license": "MIT"
}